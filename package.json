{
  "name": "@amzn/truexkeplerrefapp",
  "version": "0.0.1",
  "files": ["dist"],
  "scripts": {
    "clean": "rm -rf node_modules buildinfo.json dist",
    "start": "npx react-native start",
    "lint": "eslint src tst --ext .ts,.tsx",
    "lint:fix": "eslint src tst --ext .ts,.tsx --fix",
    "compile": "tsc -p tsconfig.json",
    "build": "npm run compile",
    "release": "npm-run-all lint test compile",
    "kepler-build": "kepler build -b Debug",
    "kepler-run": "kepler run-kepler build/vega-tv2023-aarch64-debug/truexkeplerrefapp_aarch64.vpkg com.truex.keplerrefapp.main -s",
    "kepler-build-release": "kepler build -b Release",
    "kepler-run-release": "kepler run-kepler build/vega-tv2023-aarch64-release/truexkeplerrefapp_aarch64.vpkg com.truex.keplerrefapp.main -s",
    "kepler-simulator": "kepler device simulator start",
    "kepler-fast-refresh": "kepler device start-port-forwarding --device Simulator -p 8081 --forward false"
  },
  "dependencies": {
    "@amzn/react-native-kepler": "~2.0.0",
    "react": "18.2.0",
    "react-native": "0.72.0"
  },
  "devDependencies": {
    "@amzn/kepler-cli-platform": "^0",
    "@babel/core": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@babel/traverse": "7.22.8",
    "@react-native/eslint-config": "0.72.2",
    "@react-native/metro-config": "^0.72.6",
    "@react-native-community/cli": "11.3.2",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "^28.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "babel-jest": "^28.0.0",
    "eslint": "^8.12.0",
    "jest": "^28.0.0",
    "metro-react-native-babel-preset": "^0.76.5",
    "mustache": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^28.0.0",
    "typescript": "4.8.4"
  },
  "kepler": {
    "projectType": "application",
    "appName": "TruexKeplerRefApp",
    "targets": ["tv"],
    "api": 0.1
  }
}
